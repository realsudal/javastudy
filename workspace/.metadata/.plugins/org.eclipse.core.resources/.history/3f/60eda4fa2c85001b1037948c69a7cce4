package quiz07;

import java.util.Scanner;

public class Player {

	private String name;
	private Scanner sc=new Scanner(System.in);
	
	Player(String name){
		this.name=name;
		
	}

	//κ²ν„° μ„Έν„°
	public String getName() {
		return name;
	}


	public void setName(String name) {
		this.name = name;
	}
	
	public int play() {
		System.out.println(name+"'s game start");
		System.out.println("Press Enter");
	}
	

	
	
}
