package ex02_String;

public class Quiz03 {

	public static void main(String[] args) {
		//파일명과 확장자 명 분리하기 (중요~)
		
		String fullname="20210310.jpg";
		
		String fileName=fullname.substring(0,fullname.indexOf("."));//10210310
		String exName=fullname.substring(fullname.indexOf("."));;//jpg
		
		System.out.println(fileName);
		System.out.println(exName);
		
		
		
	}

}
