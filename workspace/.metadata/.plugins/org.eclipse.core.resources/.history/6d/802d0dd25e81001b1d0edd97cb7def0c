package ex02_String;

public class Ex01_String {

	public static void main(String[] args) {
		
		/*
		 * 1. 동일한 문자열 리터럴은 자바에 의해사 최적화 되기 때문에
		 * "apple"이 하나만 생깁니다.
		 */
		String a="apple";
		String b="apple";
		System.out.println(a==b);//주소 같은지 검증
		
		/*
		 * 2. new는 무조건 새로 만든다.
		 * 바나나는 2개 
		 * 
		 * 
		 */
		String c=new String("banana");
		String d=new String("banana");
		System.out.println(c==d);
		
		
	}

}
