package ex01_random;

public class Quiz05 {

	public static void main(String[] args) {
		//5x5 크기 빙고
		//1~25를 빙고배열
		
		//final : 값을 고정
		 
		final int SIZE=5;
		
		
		int [][]bingo=new int[SIZE][SIZE];
		
		for(int i=0;i<SIZE;i++) {
			for(int j=0;j<SIZE;j++) {
				bingo[i][j]=(i*SIZE)+(j+1);
			}
			
		}
		
		//섞음
		for(int i=0;i<SIZE;i++) {
			for(int j=0;j<SIZE;j++) {
			}
			
		}
		int s=(int)((Math.random()*25)+1);
		//System.out.println(bingo[][]);
	
		
		
	}
	

}
