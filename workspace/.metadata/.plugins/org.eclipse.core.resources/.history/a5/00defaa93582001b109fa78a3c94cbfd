package Quiz04;

public class Book {
	//b3.set("홍길동전","5000");
	//b3.info();//제목 :어린왕자, 저자:미상,기격 5000 원

	String book;
	String writer="미상";
	int price=0;
	
	void set(String a,String b,int c) {
		book=a;
		writer=b;
		price=c;
	}
	
	
	
	void info() {
		
		System.out.println("제목 :"+book +"저자 :"+writer+"가격 : "+price);
	}
	
}
