package ex06_jdbc;
//source의 generate getter setter 클릭
public class Person {
	//protected 같은 패키지 또는 상속관계에 있는 다른 패키지에서만 볼수있음
	//default 같은 패키지에서만 볼수있음
	//private 클래스 내부에서만 접근 가능
	
	private String name;
	private char gender;

	public char getGender() {
		return gender;
	}

	public void setGender(char gender) {
		this.gender = gender;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
}
