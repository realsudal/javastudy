package ex02_String;

import java.util.Scanner;

public class Quiz {

	public static void main(String[] args) {
		//1.시간을 입력받아서 출력하기
		
		Scanner sc=new Scanner(System.in);
		System.out.println("시간 :");
		int h=sc.nextInt();
		
		System.out.println("분 :");
		int m=sc.nextInt();
		
		System.out.println("초 :");
		int s=sc.nextInt();
		
		System.out.println(h+"시"+ m+"분"+s+"초");
		
	}

}
