package Ex03_api;

public class Ex06_wrapper_class {

	public static void main(String[] args) {
		/*
		 * wrapper class(: bd 가공시 좋음)
		 * 1.primitive type - > reference type 변경 클래스
		 * 2.변수 ->객체로 변경해주는 클래스
		 * 3. 종류
		 * 		primitive 을 reference 로!
		 * 		1)int 	 > Integer
		 * 		2)long   > Long
		 * 		3)double > Double
		 * 		4)char 	 > Character
		 * 		5)boolean> Boolean
		 * 
		 * 4.용어
		 * 	1)Auto Boxing :primitive 값 -> reference 객체로 바꿔줌 
		 * 	2)Auto UnBoxing :reference 객체 ->primitive 값
		 */
		
		Integer a=new Integer(10);
		Integer b=new Integer("20");
		Integer c=30;//Auto Boxing :primitive 값 -> reference 객체로 바꿈 진행
		
		int total=a+b+c;//Auto UnBoxing :reference 객체 ->primitive 값  바꿈 진행
		System.out.println(total);
	}

}
