package ex01_random;

public class Quiz03 {

	public static void main(String[] args) {
		//도개걸 123 
		//윷개 6칸 
		//모모도 11칸
		
		String []n= {"도","개","걸","윷","모"};
		int move=0;
		
		while(true) {
			//프로그램이 윷 던짐
			
			int a=(int)(Math.random()*5)+0;
			System.out.print("\""+n[a]+"\"");
			
		}
		
	}

}
