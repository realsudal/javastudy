package quiz07;

public class Player {

	private String name;
	
	
	Player(String name){
		this.name=name;
		
	}

	//κ²ν„° μ„Έν„°
	public String getName() {
		return name;
	}


	public void setName(String name) {
		this.name = name;
	}
	
	public int play() {
		System.out.println(name+"'s game start");
		
	}
	

	
	
}
