package quiz02;

public class Circle {

	
	private Coordinates center;
	private double radius;
	
	Circle(Coordinates center,double radius){
		this.center=center;
		this.radius=radius;
	}
	
	
	
	
	
	
	void info(){
		System.out.print("중심 :");
		center.info();
		
		System.out.print(", 반지름 :"+radius+", 넓이 :"+Math.pow(radius, 2)*Math.PI);
	}
	
	
	
}
