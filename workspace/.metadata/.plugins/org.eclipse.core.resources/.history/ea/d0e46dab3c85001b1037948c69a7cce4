package quiz09;

public class Gun {

	private String model;
	private int bullet;
	private final int FULL_BULLET=6;
	
	
	Gun(String model,int bullet){
		this.model=model;
		this.bullet=bullet;
	}
	

	public void shoot() {
		if(bullet>0) {
			bullet--;
			System.out.println(model+"을 쐈다. 빵야("+bullet+")");
			
		}
	}
	
	
	
}
