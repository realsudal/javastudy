package Quiz05;

public class Circle {
	double radius;
	/*
	 * won.getRadius(1.5);
		System.out.println("원둘레 : "+ won.getCircum());
		System.out.println("원넓이 : "+won.getArea());
	 */
	
	void getRadius(double radius) {
		this.radius=radius;
	}
	void getCircum() {
		System.out.println("둘레 :" + radius*2);
	}
	void getArea() {
		System.out.println("원넓이 :" + Math.pow(radius, 2)*3.14);
	}
	

}
