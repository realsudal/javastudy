package quiz05;

public class Main {

	public static void main(String[] args) {
		BankAccount me=new BankAccount("1111",10000);
		BankAccount you=new BankAccount("2222",10000);
		
		
		//입금
		me.deposit(10000); //10000원 입금했습니다
		me.deposit(-10000);//불가합니다.
		
		//출금
		you.withdraw(5000);//5000원 출금
		you.withdraw(-100);//마이너스 불가
		you.withdraw(7000);//통장 잔고 부족
		
		//조회
		me.inquiry();//계좌번호: 잔액: 
		you.inquiry();//계좌번호: 잔액: 
	}

}
